{
  "manifest_version": 4,

  "name": "BART checker",
  "description": "Get arrival times for specific BART trains",
  "version": "1.0",

  "permissions": [
    "http://api.bart.gov/",
    "http://webservices.nextbus.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "bart.html"
  }
}
